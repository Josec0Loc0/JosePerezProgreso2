<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Progreso2JosePerez.JpViews"
    Shell.FlyoutBehavior="Disabled"
    Title="MauiApp2">
    <TabBar>
        <Tab Title="Recarga" Icon="icon_recarga.png">
            <ShellContent Title="Recarga" ContentTemplate="{DataTemplate local:JpRecargaPagina}" />
        </Tab>
        <Tab Title="Nombre" Icon="icon_personas.png">
            <ShellContent Title="Recarga" ContentTemplate="{DataTemplate local:JpSegundaPagina}" />
        </Tab>


    </TabBar>
    
</Shell>
